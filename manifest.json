{
  "manifest_version": 3,
  "name": "Github Repo AI Wikis",
  "version": "2.0.0",
  "description": " GitHub 源码解读快捷访问扩展，快捷访问 GitHub 仓库对应的 DeepWiki、ZreadAI、ReadmeX 页面",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*"
      ],
      "js": [
        "icons/deepwiki-icons.js",
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/deepwiki-icon.svg"
      ],
      "matches": [
        "https://github.com/*"
      ]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
